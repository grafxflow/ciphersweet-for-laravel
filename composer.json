{
    "name": "grafxflow/ciphersweet-for-laravel",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Coull",
            "email": "paul@grafxflow.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "BjornVoesten\\CipherSweet\\": "src/",
            "BjornVoesten\\CipherSweet\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0.2",
        "illuminate/config": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "paragonie/ciphersweet": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
              "BjornVoesten\\CipherSweet\\CipherSweetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
